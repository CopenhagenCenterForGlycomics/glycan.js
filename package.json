{
  "name": "glycan.js",
  "version": "0.0.1",
  "description": "A tiny library for drawing glycans",
  "main": "./js/index.js",
  "module": "./js/main.js",
  "type" : "module",
  "directories": {
    "test": "test"
  },
  "exports" : {
    ".": {
      "node": {
        "import": "./js/index.js"
      },
      "browser": {
        "import": "./js/main.js"
      },
      "default": "./js/index.mjs"
    },
    "." : "./js/main.js",
    "./sugar" : "./js/Sugar.js",
    "./iupac" : "./js/CondensedIupac.js",
    "./fragmentor" : "./js/Fragmentor.js",
    "./mass" : "./js/Mass.js"
  },
  "dependencies": {
    "debug": "^4.4.1",
    "es6-tween": "^5.5.11",
    "esm": "3.2.25"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.5",
    "core-js": "2",
    "eslint": "^7.5.0",
    "jsdom": "^24.0.0",
    "karma": "^6.4.2",
    "karma-jsdom-launcher": "^17.0.0",
    "karma-qunit": "^4.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "precommit-hook": "^3.0.0",
    "qunit": "^2.9.2",
    "raw-loader": "^2.0.0",
    "webpack": "^4.47.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start --single-run --browsers jsdom",
    "tracetest": "./scripts/trace_tests.sh",
    "build-web": "./node_modules/.bin/webpack -d",
    "build-watch-web": "./node_modules/.bin/webpack --watch",
    "server": "python ./scripts/simplehttpserver",
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "lint": "./node_modules/.bin/eslint test/**/*.js js/*.js --fix",
    "validate": "npm ls"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hirenj/glycan.js.git"
  },
  "author": "",
  "license": "TBD",
  "pre-commit": [
    "jshint-pre-commit"
  ]
}
