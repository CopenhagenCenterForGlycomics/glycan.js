{
  "name": "glycan.js",
  "version": "0.0.1",
  "description": "A tiny library for drawing glycans",
  "main": "./dist/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "debug-any-level": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "jshint": "^2.8.0",
    "karma": "^0.12.36",
    "karma-browserify": "^4.4.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-qunit": "^0.1.4",
    "precommit-hook": "^3.0.0",
    "promise-polyfill": "^2.0.2",
    "qunit": "^0.7.6",
    "qunitjs": "^1.18.0",
    "watchify": "^3.9.0"
  },
  "scripts": {
    "jshint-pre-commit": "./scripts/jshint-pre-commit.sh",
    "test": "./node_modules/karma/bin/karma start --single-run --browsers ChromeHeadless",
    "build": "./node_modules/babel-cli/bin/babel.js js -d dist",
    "build-watch": "./node_modules/babel-cli/bin/babel.js lib js --watch -d dist",
    "build-watch-web": "./node_modules/watchify/bin/cmd.js js/glycan.js -o glycan-bundle.js -t [ babelify --presets [ env ] ] -v",
    "server": "python -m SimpleHTTPServer",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hirenj/glycan.js.git"
  },
  "author": "",
  "license": "TBD",
  "pre-commit": [
    "jshint-pre-commit",
    "validate"
  ]
}
